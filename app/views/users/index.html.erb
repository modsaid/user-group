<h1>users:</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th>Description</th>
      <th>Group Id</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.age %></td>
        <td><%= user.description %></td>
	<td><%= user.group_id %></td>
	<td><%#= link_to 'Show', my_show_path(:id => user.id) %></td>
	<td><%= link_to 'edit', edit_user_path(:id => user.id) %></td>
	<td><%= link_to 'Show', user_path(:id => user.id) %></td>

        <td><%= link_to 'destroy', user, method: :delete, data: {confirm: "are you sure!"} %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New User', new_user_path %>
